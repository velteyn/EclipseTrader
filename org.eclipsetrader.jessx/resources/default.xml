    <Operators>
      <Operator name="Algo Trader 1" orderbookVisibility="5">
        <GrantedOperation name="Market Order" />
        <GrantedOperation name="Delete Order" />
        <GrantedOperation name="Limit Order" />
        <GrantedOperation name="Best Limit Order" />
      </Operator>
    </Operators>
  </Institution>
