<?xml version="1.0" encoding="UTF-8"?>
<JessXSetup>
  <GeneralParameters>
    <WorkingDirectory>./tmp</WorkingDirectory>
    <SetupFileName>Bull Market</SetupFileName>
    <XMLVersion>1.6</XMLVersion>
    <LoggingFileName>bull-market-log.xml</LoggingFileName>
    <PeriodNumber>5</PeriodNumber>
    <PeriodDuration>5000</PeriodDuration>
    <InterestRate periodCount="5">
      <PeriodRate Rate="2.5" />
      <PeriodRate Rate="2.5" />
      <PeriodRate Rate="2.5" />
      <PeriodRate Rate="2.5" />
      <PeriodRate Rate="2.5" />
    </InterestRate>
    <JoiningAfterStartup>false</JoiningAfterStartup>
  </GeneralParameters>
  <Asset type="Stock" name="AAT">
    <DividendModel periodCount="5">
      <Dividend mean="12.0" variance="1.0" />
      <Dividend mean="13.0" variance="1.0" />
      <Dividend mean="14.0" variance="1.0" />
      <Dividend mean="15.0" variance="1.0" />
      <Dividend mean="16.0" variance="1.0" />
    </DividendModel>
  </Asset>
  <Asset type="Stock" name="MSFT">
    <DividendModel periodCount="5">
      <Dividend mean="8.0" variance="1.0" />
      <Dividend mean="9.0" variance="1.0" />
      <Dividend mean="10.0" variance="1.0" />
      <Dividend mean="11.0" variance="1.0" />
      <Dividend mean="12.0" variance="1.0" />
    </DividendModel>
  </Asset>
  <Institution type="OrderMarket" name="BANK CANARIE" quotedAsset="AAT">
    <KeepOrderBook allow="true" />
    <OperationsCost>
      <Operation name="Market Order" percentageCost="1.5" minimalCost="1.0" />
      <Operation name="Delete Order" percentageCost="1.0" minimalCost="0.5" />
      <Operation name="Limit Order" percentageCost="1.0" minimalCost="0.5" />
      <Operation name="Best Limit Order" percentageCost="1.0" minimalCost="0.5" />
    </OperationsCost>
    <Operators>
      <Operator name="Operator Bull" orderbookVisibility="5">
        <GrantedOperation name="Market Order" />
        <GrantedOperation name="Delete Order" />
        <GrantedOperation name="Limit Order" />
        <GrantedOperation name="Best Limit Order" />
      </Operator>
    </Operators>
  </Institution>
  <Institution type="OrderMarket" name="Bank America" quotedAsset="MSFT">
    <KeepOrderBook allow="true" />
    <OperationsCost>
      <Operation name="Market Order" percentageCost="1.5" minimalCost="1.0" />
      <Operation name="Delete Order" percentageCost="1.0" minimalCost="0.5" />
      <Operation name="Limit Order" percentageCost="1.0" minimalCost="0.5" />
      <Operation name="Best Limit Order" percentageCost="1.0" minimalCost="0.5" />
    </OperationsCost>
    <Operators>
      <Operator name="Operator Growth" orderbookVisibility="5">
        <GrantedOperation name="Market Order" />
        <GrantedOperation name="Delete Order" />
        <GrantedOperation name="Limit Order" />
        <GrantedOperation name="Best Limit Order" />
      </Operator>
    </Operators>
  </Institution>
  <Scenario>
    <PlayerType name="Aggressive">
      <OperatorsPlayed>
        <Operator name="Operator Bull on BANK CANARIE" />
        <Operator name="Operator Growth on Bank America" />
      </OperatorsPlayed>
      <Portfolio cash="5000.0">
        <Owning asset="AAT" qtty="300" />
        <Owning asset="MSFT" qtty="300" />
      </Portfolio>
      <DividendInfo asset="AAT">
        <DividendDetailledProperties value="Dividends shown" />
        <WindowSize value="1" />
        <DisplayWindowSize value="true" />
        <DisplaySessionLength value="true" />
        <DisplayHoldingValueForExperiment value="true" />
        <DisplayHoldingValueForWindow value="true" />
        <DisplayOperationsCosts value="true" />
      </DividendInfo>
      <DividendInfo asset="MSFT">
        <DividendDetailledProperties value="Dividends shown" />
        <WindowSize value="1" />
        <DisplayWindowSize value="true" />
        <DisplaySessionLength value="true" />
        <DisplayHoldingValueForExperiment value="true" />
        <DisplayHoldingValueForWindow value="true" />
        <DisplayOperationsCosts value="true" />
      </DividendInfo>
    </PlayerType>
    <Information>
      <Information Content="Bullish environment; lower costs" Category="GLOBAL" Period="all" Time="policy" />
    </Information>
    <News>
      <Item priority="HIGH" asset="AAT" sentiment="GOOD">AAT raises full-year guidance</Item>
      <Item priority="MEDIUM" asset="AAT" sentiment="GOOD">AAT announces stock buyback program</Item>
      <Item priority="HIGH" asset="MSFT" sentiment="GOOD">MSFT cloud margins expand to record highs</Item>
      <Item priority="MEDIUM" asset="MSFT" sentiment="GOOD">MSFT acquires promising AI startup</Item>
    </News>
  </Scenario>
</JessXSetup>
